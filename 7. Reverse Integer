

int reverse(int x){
     int64_t result=0;
long int a,b=0,c=1;
  long int r=x;
    
        while(x!=0)
        { if(x>0)
        {a=x%10;
         b=(b*10)+a;}
         else
         {a=x%10;
             b=((b*10)-a);}
         
         x=x/10;
            
        }
        if(b>2147483648||b<-2147483648)
            return false;
      if(r>0)
       return b;  
    else 
        return -b;
}
